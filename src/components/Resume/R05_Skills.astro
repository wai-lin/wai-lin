---
import { resume } from "../../content/resume/data"

import BaseArticle from "./BaseArticle.astro"
---

<BaseArticle title="Skills" visible={(resume.skills?.length || 0) > 0} class="pb-4">
	<p
		set:html={resume.skills.map((skill) => {
			if (skill === "") return "<br /><br />"
			return `${skill} . `
		})}
	/>
</BaseArticle>
